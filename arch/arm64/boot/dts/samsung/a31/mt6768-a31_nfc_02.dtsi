&pio {
	nfc_det_gpio: nfc_det_gpio {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO52__FUNC_GPIO52>;
			input-enable;
			bias-disable;
		};
	};

	nfc_ven: nfc_ven {
		pins_cmd_dat{
			pinmux = <PINMUX_GPIO159__FUNC_GPIO159>;
			output-high;
			bias-disable;
		};
	};

	nfc_firm: nfc_firm {
		pins_cmd_dat{
			pinmux = <PINMUX_GPIO158__FUNC_GPIO158>;
			output-low;
			bias-disable;
		};
	};

	nfc_clk_req: nfc_clk_req {
		pins_cmd_dat{
			pinmux = <PINMUX_GPIO1__FUNC_GPIO1>;
			input-enable;
			bias-pull-down;
		};
	};

	nfc_irq: nfc_irq {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO3__FUNC_GPIO3>;
			input-enable;
			bias-pull-down;
		};
	};
};

&i2c5 {
	status = "ok";
	clock-frequency = <400000>;
	mediatek,use-open-drain;
	sec-nfc@27 {
		compatible = "sec-nfc";
		reg = <0x27>;
		interrupt-parent = <&pio>;
		interrupts = <3 IRQ_TYPE_EDGE_RISING 3 0>;
		sec-nfc,irq-gpio = <&pio 3 0>;
		sec-nfc,ven-gpio = <&pio 159 0>;
		sec-nfc,firm-gpio = <&pio 158 0>;
		sec-nfc,clk_req-gpio = <&pio 1 0>;
		sec-nfc,nfc_pvdd-supply = <&mt_pmic_vcamio_ldo_reg>;
		sec-nfc,ldo_control;
		sec-nfc,clk_req_wake;
		sec-nfc,irq_all_trigger;
		sec-nfc,nfc-det-gpio = <&pio 52 0>;
		pinctrl-names = "default";
		pinctrl-0 = <&nfc_ven &nfc_firm &nfc_det_gpio &nfc_clk_req &nfc_irq>;
	};
};
