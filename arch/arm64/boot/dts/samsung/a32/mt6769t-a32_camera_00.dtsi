	cam0_vcamd-supply = <&s2mpb03_0_l1>;
	cam0_vcamio-supply = <&s2mpb03_0_l3>;
	cam0_companion_dvdd-supply = <&s2mpb03_0_l2>;
	cam0_vcamaf-supply = <&mt_pmic_vldo28_ldo_reg>;

	// Camera1
