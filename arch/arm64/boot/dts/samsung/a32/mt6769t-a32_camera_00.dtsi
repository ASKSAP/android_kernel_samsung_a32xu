	cam1_vcamd-supply = <&s2mpb03_0_l4>;
	cam1_vcamio-supply = <&s2mpb03_0_l3>;

	// Camera2
	cam2_vcama-supply = <&mt_pmic_vcama1_ldo_reg>;
	cam2_vcamio-supply = <&s2mpb03_0_l3>;

	// Camera3
